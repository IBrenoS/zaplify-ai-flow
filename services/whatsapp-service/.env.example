# Servidor
PORT=8081
NODE_ENV=development

# Evolution API - OBRIGATÃ“RIAS
EVOLUTION_BASE_URL=http://localhost:3000
EVOLUTION_API_KEY=your_evolution_api_key_here
EVOLUTION_SESSION_ID=1
EVOLUTION_WEBHOOK_SECRET=your_webhook_secret_here_min_8_chars

# Feature Flags - OPCIONAIS
ENABLE_KAFKA=false
USE_S3=false

# Redis - OPCIONAIS
REDIS_URL=redis://localhost:6379
RATE_LIMIT_REDIS_URL=redis://localhost:6379

# Kafka - OPCIONAIS (quando ENABLE_KAFKA=true)
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=whatsapp-service

# AWS S3 - OPCIONAIS (quando USE_S3=true)
AWS_S3_BUCKET=your-bucket-name
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# OpenTelemetry - OPCIONAL
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
